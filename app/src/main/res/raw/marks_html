<script>
    $(".datepickerFromHomeWork, .datepickerToHomeWork").datepicker({
        changeYear: true,
    });

    var subjectsTr = $("#pupilListTable").find('tr');

    var marksTableTd = [];

    $(subjectsTr).each(function () {
        var subjectTd = $(this).find(".mark_subject").clone();

        if (typeof $(subjectTd).html() !== "undefined") {
            $(subjectTd).find('a, br').remove();
            marksTableTd.push($(subjectTd).html());
        }
    });

    var marksTableTh = $(subjectsTr).find("#marksSubjectCol #marksTableHead").html();

    if (typeof marksTableTh !== "undefined") {
        $("table#marksScroll tr").first().prepend('<th class="sr_visual_hidden">' + marksTableTh + '</th>');
        $("table#marksScroll tr").eq("1").prepend('<th class="sr_visual_hidden"></th>');

        var index = 0;
        var marksTable = $("table#marksScroll tr");
        $(marksTable).each(function () {
            if (typeof $(this).find('td').html() !== "undefined") {
                $(this).prepend('<td class="sr_visual_hidden">' + marksTableTd[index] + '</td>');
                index++;
            }
        });
    }
</script>


