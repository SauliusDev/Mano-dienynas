            <script type="text/javascript">


                var field2 = new LiveValidation('field2', {onlyOnSubmit: true});
                field2.add(Validate.Presence);
                var automaticOnSubmit = field2.form.onsubmit;
                field2.form.onsubmit = function() {
                var valid = automaticOnSubmit();
                if (valid) {
                document.getElementById("sent").innerHTML = "Laiškas išsiųstas";
                document.getElementById('log_in').style = 'display: none;';
                form.action = "/action/public/sendmail"
                }

                return false;
