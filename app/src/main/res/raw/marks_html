<script>
    $(document).ready(function() {
        function closeOnBackgroundRatio() {
            $('#jqPopupContainerSimpleRatio').on('click', function(e) {
                if (e.target === this) {
                    popupCloseRatio();
                }
            });
        }
        $('.dPlusNotLink').on('click mouseenter', function(e) {
            e.preventDefault();
            popupHtmlFromUrlRatio($(this).attr('href'));
            closeOnBackgroundRatio();

        });
    });

    function popupHtmlFromUrlRatio(url) {
        var ifUrl = url.indexOf('/', 0);
        if (!$("div#jqPopupContainerSimpleRatio").length) {
            createPopupHiddenElementRatio();
        }
        $("div#jqPopupContainerSimpleRatio div#jqPopupContainerHtmlSimple").html("");
        if (ifUrl > -1) {
            $.get(url, function(data) {
                $("div#jqPopupContainerSimpleRatio div#jqPopupContainerHtmlSimple").html(data);
            });
        } else {
            $("div#jqPopupContainerSimpleRatio div#jqPopupContainerHtmlSimple").html($("#" + url).html());
        }

        $('div#jqPopupContainerSimpleRatio').fadeIn('fast');
        return false;
    }

    function createPopupHiddenElementRatio() {
        var appendString = '<div id="jqPopupContainerSimpleRatio">\n\
        <div class="ratio-popup-container"> <div id="jqPopupContainerHtmlSimple"></div>\n\
        <a href="#" class="close-ratio-button" onclick="popupCloseRatio(); return false;" id="popupCloseButton"><i class="material-icons">cancel</i></a></div></div>';
        $('body').append(appendString);
    }

    function popupCloseRatio() {
        $('div#jqPopupContainerSimpleRatio').fadeOut('fast');
        $("div#jqPopupContainerSimpleRatio div#jqPopupContainerHtmlSimple").html("");
        return false;
    }
</script>
</div>
