    <script type="text/javascript">
        var timer;
        $("#languages-block").on("mouseover", function() {
            $("#top-header-content").find('.active-li').removeClass('active-li');
            clearTimeout(timer);
            openSubmenu($(this));
        }).on("mouseleave", function() {
            timer = setTimeout(
                    closeSubmenu, 300, ($(this)));
        });
        $(".dropdown-user-menu").on("mouseover click", function() {
            $("#top-header-content").find('.active-li').removeClass('active-li');
            clearTimeout(timer);
            openSubmenu($(this));
        }).on("mouseleave", function() {
            timer = setTimeout(
                    closeSubmenu, 300, ($(this)));
        });
        function openSubmenu(n) {
            $(n).addClass('active-li');

        }
        function closeSubmenu(t) {
            $(t).removeClass('active-li');
        }
        $(".dropdown-user-menu").on("click", function() {
            $(this).toggleClass('sub-open');
            $('.logged-menu-btn').removeClass('change');
            $('.side-container').removeClass('menu-open');
        });
    </script>
    <script>
        if ($('.mini').length > 0) {
            if ($('.new').length > 0) {
                var newEventsCount = $('.new').length;
                $(this).show();
                $('.new-count').text(newEventsCount);
            }

        $('.mini-ev-top, .mini-events').hover(
            function() {
                var newEvArr = [];
                $('.mini-events').show();
                $('.mini-events-inner').find('.new').each(function( index ) {
                    newEvArr.push($(this).parent().attr('data-id'));
                });

                $.ajax({
                    url: "/event/view",
                    data: { 'events': JSON.stringify(newEvArr) }
                }).done(function( msg ) {
                    if (true == msg) {
                        $('.new-count').remove();
                        $('.mini-events-inner').find('.new').each(function( index ) {
                            $(this).removeClass("new");
                        });
                    }
                });
            }, function() {
                $('.mini-events').hide();
            }
        );

        $('.mini-text').each(function(index) {
            getUrl($(this));
        });
    }

    function getUrl(elem) {
        var exp = /(\b(https?|ftp|file):\/\/[-A-Z0-9+&@#\/%?=~_|!:,.;]*[-A-Z0-9+&@#\/%=~_|])/ig;

        if (exp.test(elem.html())) {
            var url = elem.html();
            elem.parent().parent().parent().attr('href', url);
        }
    }
    </script>
</header>








