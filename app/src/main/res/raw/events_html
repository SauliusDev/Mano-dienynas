    <script>
        const mainButtoon = document.getElementById('mainButton');
        let allImages = document.querySelectorAll('img');
        mainButtoon.addEventListener('click', event => {
            allImages.forEach(node => {
            node.src = node.dataset.secret;
            });
        });

        if (typeof jQuery.fn.live == 'undefined' || !(jQuery.isFunction(jQuery.fn.live))) {
            jQuery.fn.extend({
                live: function (event, callback) {
                   if (this.selector) {
                        jQuery(document).on(event, this.selector, callback);
                    }
                }
            });
        }

        var previewBtn = '<button type="button" data-preview="true" class="whiteButton ev-preview" style="margin-top: 10px; cursor: pointer;" spellcheck="true">\n\
            <i class="material-icons" style="color:#DA7115" title="Taisyti">edit</i><span>Peržiūrėti</span></button>';

        $("#event_save").parent().addClass('modal-footer');
        $('#myModal').find('.modal-footer').prepend(previewBtn);
    </script>
