    $('.event-image').each(function() {
      if ($(this).attr("id") != "5e903593625ee4cb4c4996f3") {
        $(this).click(function() {
            var popupWrap = $('.ev-img-popup-wrap');
            var evImgId = $(this).attr('id');
            var evImgSrc = $(this).attr('src');
            if (popupWrap.length == 0) {
                $('body').append("<div class='ev-img-popup-wrap'><img src=" + evImgSrc + " id=" + evImgId + "/></div>");
            } else {
                $('.ev-img-popup-wrap img').attr('id', evImgId);
                $('.ev-img-popup-wrap img').attr('src', evImgSrc);
                popupWrap.fadeIn('slow');
            }
        });
        }
    });

