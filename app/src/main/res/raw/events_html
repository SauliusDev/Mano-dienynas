    $(document).on('click', '.recipient_groups-choices div', function() {
        var form = $(this).closest('form');
        var text = $(this).text();
        var group = $(this).attr('data-value');
        var markAll = form.find('.mark-all input');

        form.find('.recipient_groups-field').attr('data-group', group);
        form.find('.recipient_groups-field .recipient-groups-holder').text(text);
        form.find('#event_recipients').show();
        form.find('#event_recipients input').parent().hide();
        form.find('.group_' + group).parent().show();
        form.find('.mark-all').show();
        markAll.prop('checked', true);
    });

    $(document).on('click', '.recipient_class-choices div', function(ev) {
        var form = $(this).closest('form');
        var text = $(this).text();
        var pupilClass = $(this).attr('data-value');

        $(this).parent().hide();
        form.find('#event_recipients').show();
        form.find('.recipient-class-holder').text(text);
        form.find('.recipient-class-holder').attr('data-class', $(this).attr('data-value'));
        form.find('#event_recipients').children().hide();

        $.each(pupilArr[pupilClass], function(index, value) {
            form.find('#event_recipients_' + this).parent().show();
        });
        var countVisible = $('#event_recipients input:visible').length;
        if(countVisible < 1){
            $('#event_recipients').css('border','none');
        } else{
            $('#event_recipients').css('border','1px solid darkgrey');
        }
    });

    $(document).on('click', '.ev-preview', function(ev) {
        var modal = $(this).closest('.modal');
        var text = tinyMCE.activeEditor.getContent();
        var header = modal.find('#event_header').val();
        var previewText = modal.find('.ev-preview-text');
        var img = modal.find('.ev-img').attr('src');
        var dataPreview = $(this).attr('data-preview');

        modal.find('.preivew-img').attr('src', '#');

        if (dataPreview == 'true') {
            $(this).find('span').html('Redaguoti');
            $(this).attr('data-preview', 'false');
        } else {
            $(this).find('span').html('Per\u017Ei\u016Br\u0117ti');
            $(this).attr('data-preview', 'true');
        }

        if (img != null && img != '#') {
            modal.find('.preivew-img').attr('src', img);
            modal.find('.preivew-img').show();
            modal.find('.preview-link').hide();
        } else if (modal.find('.event-image-bg').length > 0) {
            modal.find('.preivew-img').attr('src', modal.find('.event-image-bg').attr('data-src'));
            modal.find('.preivew-img').show();
            modal.find('.preview-link').hide();
        } else {
            modal.find('.preview-link').show();
            modal.find('.preivew-img').hide();
        }

        modal.find('.preview-panel, .form-group').toggle();
        modal.find('.ev-preview-header').html(header);
        previewText.html(text);
        urlifyPreview(previewText);
        modal.find('.ev-preview-files').html('');
        modal.find('.ev-preview-attached').css('display', 'none');

        if ((modal.find('.filename').length > 0 && modal.find('.ev-img').length < 1) ||
            (modal.find('.filename').length > 1 && modal.find('.ev-img').length > 0) )
        modal.find('.ev-preview-attached').css('display', 'block');

        modal.find('.filename').each(function() {
            if ($(this).css('display') == "none") return;

            var filename = $(this).find('span').html();
            var fileType = filename.slice((Math.max(0, filename.lastIndexOf(".")) || Infinity) + 1);

            if ((fileType == "docx") || (fileType == "doc")) {
                modal.find('.ev-preview-files').append('<img src="/layout/images/document-word.png">');
            } else if ((fileType == "jpg") || (fileType == "gif") || (fileType == "png")) {
                modal.find('.ev-preview-files').append('<img src="/layout/images/document-image.png">');
            } else if ((fileType == "xls") || (fileType == "xlsx")) {
                modal.find('.ev-preview-files').append('<img src="/layout/images/document-excel.png">');
            } else if ((fileType == "pdf")) {
                modal.find('.ev-preview-files').append('<img src="/layout/images/document-pdf.png">');
            } else {
                modal.find('.ev-preview-files').append('<img src="/layout/images/document.png">');
            }
        });
    });

    function urlifyPreview(elem) {
        var exp = /(\b(https?|ftp|file):\/\/[-A-Z0-9+&@#\/%?=~_|!:,.;]*[-A-Z0-9+&@#\/%=~_|])/ig;
        var text = elem.html();
        var previewBlock = elem.closest('.preview-panel');

        if (exp.test(text)) {
            var text2 = text.replace(exp, "<a href='$1' class='text-link'>$1</a>");
            var url = elem.html().match(exp)[0];

            elem.html(text2);

            if (previewBlock.find('.preivew-img').attr('src') != '#') {
                return true;
            }

            $.ajax({
                type: "POST",
                url: '\/event\/get_content',
                data: {url: url},
                success: function(data) {
                    var evLink = elem.parent().find('.preview-link');
                    var hasLink = false;
                    var youtube = false;

                    evLink.prop('href', url);
                    evLink.attr('data-ready', true);

                    $(data).each(function(index) {
                        if ($(this).attr('property') == 'og:site_name' && $(this).attr('content') == 'YouTube') {
                            youtube = true;
                        }
                        if (youtube && $(this).attr('property') == 'og:video:url') {
                            if ($(this).attr('content').includes('embed')) {
                                hasLink = true;
                                evLink.find('.ev-link-img')
                                        .html('<iframe src="' + $(this).attr('content') + '"></iframe>');
                            }
                        }
                        if ($(this).attr('property') == 'og:image' && elem.find('.link-image').length < 1 && !youtube) {
                            var imgSrc = $(this).attr('content');
                            hasLink = true;
                            evLink.find('.ev-link-img').html('<img class="link-image" src="' + imgSrc + '" style="width:100px;">');
                        }
                        if ($(this).attr('property') == 'og:title' && elem.find('.link-title').length < 1) {
                            var title = $(this).attr('content');
                            hasLink = true;
                            evLink.find('.ev-link-title').html('<span class="link-title">' + title + '</span>');
                        }
                        if (!youtube && $(this).attr('property') == 'og:description' && elem.find('.link-body').length < 1) {
                            var body = $(this).attr('content');
                            hasLink = true;
                            evLink.find('.ev-link-body').html('<span class="link-body">' + body + '</span>');
                        }
                    });

                    if (youtube) {
                        evLink.removeClass('col-md-8');
                        evLink.css('border', 'none');
                    }

                    if (hasLink == true) {
                        elem.find('.text-link')[0].remove();
                        evLink.show();
                    }
                }
            });
        }
    }

    $(document).on('click', '#new_ev_btn', function(ev) {
        $('#form_create_event')[0].reset();
        $('#myModal').find('.recipient-blocks').html('');
        $('#myModal').find('#event_recipients').hide();
        $('#myModal').find('.mark-all').hide();
        $('#myModal').find('.ev-img').attr('src', '#');
        $('#myModal').find('.ev-img').attr('alt', '');
        $('#myModal').find('.remove-picture').hide();
        $('#myModal').find('.filename').remove();
        $('.preview-create').find('.ev-link-img, .ev-link-title, .ev-link-body').html('');
        $('#myModal').find('.saved-recipients').html('');
        $('#myModal').find('.saved-class').html('');
        $('#myModal').find('.ev-preview-header').html('');
        $('#myModal').find('.ev-preview-text').html('');
        $('#myModal').find('.ev-preview-files').html('');
        $('#myModal').find('.ev-preview-attached').hide();
        $('#myModal').find('.preview-link').hide();
        $('#myModal').find('.preivew-img').attr('src', '#');
        var newForm = $('#form_create_event');
        addedRecipients(newForm);
    });

    function appendSavedRecipients(ev){
        currentEvRecipients = [];

        ev.find( ".remove-stored-recipient" ).each(function( index ) {
            var globalId = $(this).attr('data-globalId');

            if (!currentEvRecipients.includes(globalId)) {
                currentEvRecipients.push(globalId);
            }
        });
    }

    function showSavedClasses(form, group){
        var recipients = $('.group_' + group + ':checked');
        var recClass = {};

        form.find('.saved-class').attr('data-class', 'true');

        recipients.each(function() {
            var glId = $(this).val();
            var recipient = $(this);

            $.each(pupilArr, function(pupilClass, ids) {
                if(pupilArr[pupilClass].includes(glId)) {
                    var className = $('[data-value=' + pupilClass + ']').html();

                    if (!recClass.hasOwnProperty(className)) {
                        recClass[className] = [];
                        form.find('.saved-class').append('<div class="saved-class-' + pupilClass + ' saved-group-holder">' + className +
                        '<button type="button" class="remove-saved-class" data-class="' +
                        pupilClass + '"><i class="material-icons">clear</i></button></div>');
                    }

                    recClass[className].push(recipient);
                }
            });
        });
    }

    $(document).on('click', '.remove-saved-group', function(ev) {
        var form = $(this).closest('form');
        var group = '.' + $(this).attr('data-group');
        var el = $(this);
        var recipients = form.find('input[name="event[recipients][]"]:checked');

        form.find(group + ':checked').each(function( index ) {
            var evRecipient = form.find('.recipient-' + $(this).attr('id')).attr('data-recipient');

            deleteEventRecipient(evRecipient);
            $(this).prop('checked', false);
            form.find('.recipient-' + $(this).attr('id')).remove();
        });

        form.find('#event_mark_all_0').prop('checked', false);
        el.parent().remove();
        addedRecipients(form);
    });

    $(document).on('click', '.remove-saved-class', function() {
        var form = $(this).closest('form');
        var clId = $(this).attr('data-class');
        var el = $(this);
        var recipients = form.find('input[name="event[recipients][]"]:checked');
        var group = form.find('.recipient_groups-field').attr('data-group');

        pupilArr[clId].forEach(function(glId) {
            var evRecipient = form.find('.recipient-event_recipients_' + glId).attr('data-recipient');

            deleteEventRecipient(evRecipient);
            form.find('#event_recipients_' + glId).prop('checked', false);
            form.find('.recipient-event_recipients_' + glId).remove();
        });

        if (form.find('.group_' + group + ':checked').length < 1) {
            form.find('.saved-group-' + group).remove();
        }

        el.parent().remove();
        addedRecipients(form);
    });

    function addRecipientsGroupBlock(input, checked) {
        var form = input.closest('form');
        var group = input.attr('class').split('_')[1];

        if (form.find('.saved-group-' + group).length == 0 && checked) {
            form.find('.saved-recipients').append('<div class="saved-group-' + group + ' saved-group-holder">' + translations[group] +
                '<button type="button" class="remove-saved-group" data-group="group_' + group
                + '"><i class="material-icons">clear</i></button></div>');
        } else if (!checked && $('.' + input.attr('class') + ':checked').length < 1) {
            form.find('.saved-group-' + group).remove();
        }

        if (form.find('#recipient_class_block').is(":visible") && form.find('.recipient-class-holder').attr('data-class') != 'false') {
            var pupilClass = form.find('.recipient-class-holder').attr('data-class');
            var className = form.find('.recipient-class-holder').html();

            if (form.find('.saved-class-' + pupilClass).length < 1 && pupilClass != 'null') {
                form.find('.saved-class').append('<div class="saved-class-' + pupilClass + ' saved-group-holder">' + className +
                    '<button type="button" class="remove-saved-class" data-class="' +
                    pupilClass + '"><i class="material-icons">clear</i></button></div>');
            }

            if (!checked && form.find('input[pupil-class=' + pupilClass + ']:checked').length < 1) {
                form.find('.saved-class-' + pupilClass).remove();
            }
        }
        addedRecipients(form);
    }

    function addedRecipients(form){
        var formId = form.attr('id');
        var countAddedRecipients = $('#'+formId+' .recipient-holder').length;
    }

    $(document).on('click', '.recipient_groups-choices div', function(ev) {
        var group = $(this).attr('data-value');
        var form = $(this).closest('form');
        var countGroup = form.find('.group_' + group).length;

        if (countGroup == 0) {
            $.ajax({
                type: "POST",
                url: '\/event\/recipients_get',
                data: {'group': group},
                success: function(data) {
                    var markAll = form.find('.mark-all input');

                    $.each(data, function( index, value ) {
                        var checked = currentEvRecipients.includes(index) ? 'checked' : '';
                        form.find('#event_recipients').append(
                            '<div style=""><input type="checkbox" id="event_recipients_' +
                            index + '" name="event[recipients][]" class="recipient-checkbox group_' +
                            group + '" value="' + index + '-' +
                            group + '" spellcheck="true" ' + checked + '><label for="event_recipients_' + index + '">' +
                            value + '</label></div>');
                    });

                    var countVisible = form.find('#event_recipients input:visible').length;
                    if(countVisible < 1){
                        form.find('#event_recipients').css('border','none');
                    } else {
                        form.find('#event_recipients').css('border','1px solid darkgrey');
                    }

                    form.find('#event_recipients input:visible').each(function(index) {
                        if (!$(this).prop('checked')) {
                            markAll.prop('checked', false);
                        }

                        let
                        userName = $(this).parent().find('label').html();
                        if (userName.length < 3) {
                            $(this).parent().hide()
                        }
                    });
                }
            });
        } else {
            form.find('.recipient-checkbox').hide();
            form.find('.group_' + group).show();
        }

        $('#event_mark_all_0').prop('checked', false);
    });
</script>
<script>
    $(document).on('click', '.load-comments', function() {
        var evId = $(this).attr('data-ev-id');
        var offset = $(this).attr('data-offset');
        var commentBlock = $(this).parent().parent().find('.ev-comments');
        var route = '\/event\/get_comments\/id';
        var url = route.replace("id", evId);
        $(this).parent().hide();

        $.ajax({
            type: "POST",
            url: url,
            data: {evId: evId, offset: offset},
            success: function(data) {
                commentBlock.html(data);
            }
        });
    });
</script>
<script>
    function tinyMceInit(selector = ".tinymce") {
        tinymce.init({
            toolbar: "bold italic underline strikethrough | subscript superscript | bullist numlist | alignleft aligncenter alignright alignjustify | fontselect fontsizeselect | undo redo | paste",
            menubar: false,
            statusbar: false,
            paste_auto_cleanup_on_paste: true,
            paste_remove_styles: true,
            paste_remove_styles_if_webkit: true,
            paste_strip_class_attributes: true,
            paste_as_text: true,
            selector: selector,
            themes: 'modern',
            plugins: "advlist autolink lists link image charmap print preview anchor paste",
            language : ""
        });
    }

    function init() {
        bindUrlify();
    }

    init();
    tinyMceInit();
</script>
    <script>
(function($) {
  var selectors = [];

  var check_binded = false;
  var check_lock = false;
  var defaults = {
    interval: 250,
    force_process: false
  };
  var $window = $(window);

  var $prior_appeared = [];

  function appeared(selector) {
    return $(selector).filter(function() {
      return $(this).is(':appeared');
    });
  }

  function process() {
    check_lock = false;
    for (var index = 0, selectorsLength = selectors.length; index < selectorsLength; index++) {
      var $appeared = appeared(selectors[index]);

      $appeared.trigger('appear', [$appeared]);

      if ($prior_appeared[index]) {
        var $disappeared = $prior_appeared[index].not($appeared);
        $disappeared.trigger('disappear', [$disappeared]);
      }
      $prior_appeared[index] = $appeared;
    }
  }

  function add_selector(selector) {
    selectors.push(selector);
    $prior_appeared.push();
  }

  // "appeared" custom filter
  $.expr[':'].appeared = function(element) {
    var $element = $(element);
    if (!$element.is(':visible')) {
      return false;
    }

    var window_left = $window.scrollLeft();
    var window_top = $window.scrollTop();
    var offset = $element.offset();
    var left = offset.left;
    var top = offset.top;
    if (top + $element.height() >= window_top &&
        top - ($element.data('appear-top-offset') || 0) <= window_top + $window.height() &&
        left + $element.width() >= window_left &&
        left - ($element.data('appear-left-offset') || 0) <= window_left + $window.width()) {

    if($(element).attr('data-viewed') == 'false') {
        var newEvents  = $('[data-viewed=false]').length;
        var getLastId = $('[data-viewed=false]')[newEvents-1].id;
        createViewed(element);
        if(getLastId == element.id){
            $('.menu_img_events .navParentASpan span').fadeOut('slow');
            $('.mini-ev-top .new-count').fadeOut('slow');
        }
    }

      return true;
    } else {
      return false;
    }
  };

  $.fn.extend({
    // watching for element's appearance in browser viewport
    appear: function(options) {
      var opts = $.extend({}, defaults, options || {});
      var selector = this.selector || this;
      if (!check_binded) {
        var on_check = function() {
          if (check_lock) {
            return;
          }
          check_lock = true;

          setTimeout(process, opts.interval);
        };

        $(window).scroll(on_check).resize(on_check);
        check_binded = true;
      }

      if (opts.force_process) {
        setTimeout(process, opts.interval);
      }
      add_selector(selector);
      return $(selector);
    }
  });

  $.extend({
    // force elements's appearance check
    force_appear: function() {
      if (check_binded) {
        process();
        return true;
      }
      return false;
    }
  });
})(function() {
  if (typeof module !== 'undefined') {
    // Node
    return require('jquery');
  } else {
    return jQuery;
  }
}());
</script>

