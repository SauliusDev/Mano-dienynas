    <script>
    var currentEvRecipients = [];
    var protectClick = true;
    var pupilArr = [];
    var pupilRole = 1;
    var parentRole = 2;
    var imgExt = ["image/jpeg", "image/gif", "image/svg+xml", "image/png"];
    var translations = {
        1: "Mokiniai",
        2: "T\u0117vai",
