    $(".event-holder").each(function() {
        var commCount = $(this).find(".media").length;
        $(this).find("span.comment-count").append(commCount);
        $(this).find("#event_add_file").wrapInner('<span></span>').prepend('<i class="material-icons mdGreenText">add_circle_outline</i>');
        $(this).find('#event_save').wrapInner('<span></span>').prepend('<i class="material-icons mdBlue">save</i>');
    });

    $(document).on('click', '#image-btn', function(event) {
        $("#event_image").click();
    });

    $(document).on('click', '.image-edit-btn', function(event) {
        var form = $(this).parent().parent();
        form.find("#event_image").click();
    });

    $(document).on('change', '#event_image', function(event) {
        var form = $(this).parent().parent();
        var fileName = $(this).val().split('/').pop().split('\\').pop();

        form.find('.img-name').html(fileName);
    });

    $(document).ready(function() {
        $("#event_add_file").wrapInner('<span></span>').prepend('<i class="material-icons mdGreenText">add_circle_outline</i>');
        $('#event_save').wrapInner('<span></span>').prepend('<i class="material-icons mdBlue">save</i>');
    });

    $(document).on('keyup click', '#event_recipient_search', function(ev) {
        if ($(this).val().length > 2) {
            var form = $(this).parent().parent().parent();
            var search = $(this).val();
            var formSearched = form.find('.searched');

            formSearched.html('');

            form.find('#event_recipients label').each(function(index) {
                var name = $(this).text().toLowerCase();
                if (name.includes(search) && $('.' + $(this).attr('for')).length < 1) {
                    var searched = '<div class="' + $(this).attr('for') + '">' + $(this).text() + '</div>';

                    formSearched.show();
                    formSearched.append($(searched));
                }
            });
        }
    });

    $(document).on('click', '.searched div', function(ev) {
        var checkId = $(this).attr('class');

        $('#' + checkId).prop('checked', true);
        $('.searched').html('');
        $('.searched').hide();
        addRecipientBlock($('#' + checkId));
    });

    $(document).on('click', '.recipient-info', function(ev) {
        var form = $(this).closest('form');
        var infoLink = $(this);
        var user = infoLink.attr('data-user');
        var type = form.find('.recipient_groups-field').attr('data-group');

        $.ajax({
            type: "POST",
            url: '\/user\/school_user_info',
            data: {'user': user, 'type': type},
            success: function(data) {
                infoLink.attr('data-content', data);
                infoLink.popover('show');
            }
        });
    });

    $(document).on('click', '.recipient_groups-choices div', function(ev) {
        var form = $(this).closest('form');
        var group = $(this).attr('data-value');

        form.find('#recipient_class_block').hide();
        form.find('.saved-class').hide();
        $(this).parent().hide();

        if (pupilRole == group || parentRole == group) {
            form.find('#recipient_class_block').show();

            $.ajax({
                type: "POST",
                url: '\/event\/get_classes_recipients',
                data: {'role': group},
                success: function(data) {
                    pupilArr = data;
                    form.find('.saved-class').show();

                    $.each(pupilArr, function(pupilClass, ids) {
                        $.each(ids, function(index, glId) {
                            form.find('#event_recipients_' + glId).attr('pupil-class', pupilClass);
                        });
                    });

                    if(form.find('.saved-class').attr('data-class') == "false")showSavedClasses(form, group);
                }
            });
        }
    });

    $("<div class='checkAppend' data-ev-count='" + $('.ev-count-cl').length + "' data-gl-ev-count='" + $('.glEv-count').length + "'></div>")
            .insertAfter($('.ev-count-cl').last());

    $(window).on('scroll', function() {
        $('.event-holder').is(':appeared');

        if ($('.checkAppend').is(':appeared')) {
            appendEvents([$('.checkAppend').attr('data-ev-count'), $('.checkAppend').attr('data-gl-ev-count')]);
        }
    });

    $(document).on('submit', '#form_create_event', function(e) {
        var form = $(this);

        e.preventDefault();

        if (form.find('input[name="event[recipients][]"]:checked').length > 0) {
            $.ajax({
                type: "POST",
                url: '\/event\/build',
                contentType: false,
                cache: false,
                processData: false,
                data: new FormData(this),
                success: function(data) {
                    if (data == false) {
                        $(this).val('');
                        alert('Must\u0020be\u0020\u002Ajpeg,\u0020\u002Agif,\u0020\u002Asvg,\u0020\u002Apng');
                    } else {
                        $('#myModal').modal('hide');
                        $(data).prependTo(".event-row");
                        var eventId = $(".event-holder").attr("id");
                        $(".event-holder#" + eventId + " .ev-file-link").each(function() {
                            var link = $(this).attr('href');
                            var fileType = link.slice((Math.max(0, link.lastIndexOf(".")) || Infinity) + 1);
                            if ((fileType == "docx") || (fileType == "doc")) {
                                $(this).children().attr({src: '/layout/images/document-word.png'});
                            } else if ((fileType == "jpg") || (fileType == "gif") || (fileType == "png")) {
                                $(this).children().attr({src: '/layout/images/document-image.png'});
                            } else if ((fileType == "xls") || (fileType == "xlsx")) {
                                $(this).children().attr({src: '/layout/images/document-excel.png'});
                            } else if ((fileType == "pdf")) {
                                $(this).children().attr({src: '/layout/images/document-pdf.png'});
                            } else {
                                $(this).children().attr({src: '/layout/images/document.png'});
                            }
                        });
                        $('<i class="material-icons mdGreenText">question_answer</i>').prependTo("#event_comment_save");
                        $('#' + eventId + " #event_save").wrapInner('<span></span>').prepend('<i class="material-icons mdBlue">save</i>');
                        $('#' + eventId + " #event_add_file").wrapInner('<span></span>').prepend('<i class="material-icons mdGreenText">add_circle_outline</i>');
                        $('#form_create_event')[0].reset();
                        $('#myModal').find('.recipient-blocks').html('');
                        $('#myModal').find('#event_recipients').hide();
                        $('.mark-all').hide();
                        $('#myModal').find('.ev-img').attr('src', '#');
                        $('#myModal').find('.ev-img').attr('alt', '');
                        $('#myModal').find('.remove-picture').hide();
                        $('#myModal').find('.filename').remove();
                        $('.preview-create').find('.ev-link-img, .ev-link-title, .ev-link-body').html('');
                        $('#myModal').find('.saved-recipients').html('');
                        $('#myModal').find('.saved-class').html('');
                        init();
                        $('#' + eventId + " #event_text").prop('id', 'textarea_' + eventId);
                        tinymce.execCommand('mceAddEditor', true, 'textarea_' + eventId);
                    }
                }
            });
        } else {
            alert('Nepasirinkote\u0020gav\u0117j\u0173\u0020grup\u0117s\u0021');
        }
    });

    $(document).on('mouseover', '.like, .follower', function() {
        var el = this;
        var namesCount = $($(el).children('div')).children().length;

        if (namesCount > 0) {
            $($(el).children('div')).show();

            $(el).mouseout(function() {
                $($(el).children('div')).hide();
            });
        }
    });

    $(document).on('click', '.editor', function(ev) {
        var eventBlock = $(this).closest('.event-holder');
        var evId = eventBlock.attr('id').substring(6);
        var modal = $('#edit_' + evId);
        eventBlock.find('#recipient_class_block').hide();
        eventBlock.find( ".remove-stored-recipient" ).each(function( index ) {
            eventBlock.find('#' + $(this).attr('data-recipient')).prop('checked', true);
        });

        modal.find('.edit-files-area').insertAfter(modal.find('#event_add_file').parent());
        eventBlock.find('.saved-class').hide();
        appendSavedRecipients(eventBlock);
        addedRecipients(modal);
    });

    $(document).on('click', '.comment-edit-btn', function() {
        var media = $(this).parent().parent();

        $($(media.children()[2]).children()[2]).toggle();
        $(media.children()[3]).toggle();
    });

    $(document).on('submit', '.form_edit_event', function(e) {
        var form = $(e.target);
        var evId = form.attr('data-ev-id');
        var route = '\/event\/build\/id';
        var url = route.replace("id", evId);

        e.preventDefault();
        $('#edit_' + evId).modal('hide');

        $.ajax({
            type: "POST",
            url: url,
            contentType: false,
            cache: false,
            processData: false,
            data: new FormData(e.target),
            success: function(data) {
                $('#event_' + evId).replaceWith(data);
                var commCount = $('#event_' + evId + " .media").length;
                $('#event_' + evId + ' .comment-count').append(commCount);
                $('<i class="material-icons mdGreenText">question_answer</i>').prependTo('#event_' + evId + " #event_comment_save");
                $('#event_' + evId + " #event_save").wrapInner('<span></span>').prepend('<i class="material-icons mdBlue">save</i>');
                $('#event_' + evId + " #event_add_file").wrapInner('<span></span>').prepend('<i class="material-icons mdGreenText">add_circle_outline</i>');
                init();
                showFileImg($('#event_' + evId));
            }
        });
    });

    $(document).on('click', '.like, .follower', function(e) {
        var btn = $(this);
        var obj = btn.attr('data-object');
        var evId = btn.attr('data-event');
        var span = $(btn.children('span')[0]);
        var namesList = $(btn.children('div')[0]);
        var route = '\/event\/like_follow_view\/object\/eventId';
        var url = route.replace('object', obj).replace('eventId', evId);

        if (protectClick) {
            protectClick = false;

            $.ajax({
                type: "POST",
                url: url,
                success: function(data) {
                    if (data[0] == false) {
                        btn.attr('data-active', true);
                        btn.toggleClass('btn-default btn-primary');
                        span.text(' ' + (parseInt(span.text()) + 1));
                        namesList.prepend('<div class="userName">' + data[1] + '</div>')
                    } else {
                        btn.attr('data-active', true);
                        btn.toggleClass('btn-primary btn-default');
                        span.text(' ' + (parseInt(span.text()) - 1));
                        btn.find(".userName:contains('" + data[1] + "')").remove();

                        /*if ($(btn.children()[2]).length < 1) {
                         $(btn.children()[1]).hide();
                         }*/
                    }

                    protectClick = true;
                }
            });
        }
    });

    $(document).on('submit', '.form-comment', function(e) {
        var submitBtn = $($(e.target).children().children()[1]).children();
        var form = $(e.target);
        var evId = form.attr('data-ev-id');
        var route = '\/event\/create_comment\/id';
        var url = route.replace("id", evId);

        submitBtn.prop('disabled', true);
        e.preventDefault();

        $.ajax({
            type: "POST",
            url: url,
            data: form.serialize(),
            success: function(data) {
                var comments = $(data).find('.ev-comments');
                var commCount = $('#event_' + evId).find(".media").length + 1;
                form.trigger("reset");
                $('#event_' + evId).find('.ev-comments').html(comments.html());
                $('#event_' + evId).find('.comment-count').html(commCount);
                submitBtn.prop('disabled', false);
            }
        });
    });

    $(document).on('submit', '.form-comment-edit', function(e) {
        var form = $(this);
        var evId = form.parent().parent().parent().parent().attr('id');
        var id = form.attr('data-comment-id');
        var route = '\/event\/comment_edit\/id';
        var url = route.replace("id", id);

        e.preventDefault();

        $.ajax({
            type: "POST",
            url: url,
            data: form.serialize(),
            success: function(data) {
                var editPost = $('#' + evId).replaceWith(data);
                var commCount = editPost.find(".media").length;
                $('#' + evId).find('.comment-count').append(commCount);
                init();
            }
        });
    });

    $(document).on('click', '#event_add_file', function(e) {
        var form = $(this).closest('form');
        var savedFiles = form.find('.ev-file-edit');
        var newFiles = form.find('.new-file');
        var leftSpace = newFiles.length - savedFiles.length;

        if (form.find('.file-shown').length < leftSpace) {

            form.find('.new-file').each(function(index) {
                if (!$(this).hasClass('file-shown')) {
                    var inputId = $(this).attr('id');

                    $(this).click();
                    $(this).addClass('file-shown');

                    return false;
                }
            });
        }
    });

    $(document).on('click', '.ev-file-btn', function(e) {
        var form = $(this).parent().parent().parent();
        form.find('#' + $(this).attr('data-file')).click();
    });

    $(document).on('click', '.remove-file', function(e) {
        var fileInputId = $(this).attr('data-to-hide');
        var form = $(this).parents('form');
        var fileInput = form.find('#' + fileInputId);

        $(this).parent().remove();
        fileInput.val('');
        fileInput.removeClass('file-shown');

        if (fileInput.hasClass('image-input')) {
            form.find('.remove-picture').hide();
            fileInput.removeClass('image-input');
            form.find('.ev-img').hide();
            form.find('.ev-img').attr('src', '#');
            setImage = false;
        }
    });

    $(document).on('change', '.new-file', function(e) {
        var form = $(this).parent().parent().parent();
        var inputId = $(this).attr('id');
        var fileName = $(this).val().split('/').pop().split('\\').pop();

        readURL(this);

        form.find('#event_add_file').parent().before('<div class="filename"><span>' + fileName + '</span><a href="#" class="remove-file" \n\
            data-to-hide="' + inputId + '" style="margin-left:5px;"><i class="material-icons">highlight_off</i></a></div>');
    });

    $(document).on('click', '.delete', function(e) {
        var delId = $(this).attr('data-to-delete');

        if (confirm("Ar\u0020tikrai\u0020norite\u0020i\u0161trinti\u0020\u0161\u012F\u0020\u012Fra\u0161\u0105\u003F")) {
            $.ajax({
                type: "POST",
                url: '\/event\/delete',
                data: {'delEvent': delId},
                success: function(data) {
                    $($("#event_" + delId)).fadeOut(300, function() {
                        $(this).remove();
                    });
                }
            });
        }
    });

    $(document).on('click', '.del-recipient', function(ev) {
        var form = $(this).parent().parent().parent().parent().parent().parent();
        var checkbox = form.find('#' + $(this).attr('data-recipient'));

        checkbox.prop('checked', false);
        $(this).parent().remove();
        addedRecipients(form);
    });

    $(document).on('change', '#event_pupil_class', function(ev) {
        var form = $(this).parent().parent().parent();
        var markAll = form.find('.mark-all input');

        form.find('.mark-all').show();
        markAll.prop('checked', true);

        form.find('#event_recipients input:visible').each(function(index) {
            if (!$(this).prop('checked')) {
                markAll.prop('checked', false);
            }

            let
            userName = $(this).parent().find('label').html();
            if (userName.length < 3) {
                $(this).parent().hide()
            }
        });
    });

    $(document).on('change', '.mark-all input', function(ev) {
        var form = $(this).parent().parent().parent();
        var checked = $(this).prop('checked');
        var group = form.find('.recipient_groups-field').attr('data-group');

        form.find('#event_recipients input:visible').each(function(index) {
            if ($(this).prop('checked') != checked) {
                $(this).prop('checked', checked);
                addRecipientBlock($(this));
            }
        });
        var countVisible = form.find('#event_recipients input:visible').length;
        if(countVisible < 1){
            $('#event_recipients').css('border','none');
        } else{
            $('#event_recipients').css('border','1px solid darkgrey');
        }

        if (checked) {
            form.find('.saved-recipients').append('<div class="saved-group-' + group + ' saved-group-holder">' + translations[group] +
                '<button type="button" class="remove-saved-group" data-group="group_' + group
                + '"><i class="material-icons">clear</i></button></div>');
        } else {
            form.find('.saved-group-' + group).remove();
        }
    });

    $('body').on('change', '#event_recipients input', function() {
        var checked = $(this).prop('checked');
        var group = $(this).attr('class');

        if (!checked) {
            deleteEventRecipient($('.recipient-event_recipients_' + $(this).val() + '.existing-recipient').attr('data-recipient'));

            if ($('.' + group + ':checked').length < 1) {
                $('.saved-group-' + group.split('_')[1]).remove();
            }
        }

        addRecipientsGroupBlock($(this), checked);
        addRecipientBlock($(this));
    });

    $(document).on('click', '.remove-stored-recipient', function(ev) {
        deleteEventRecipient($(this).parent().attr('data-recipient'));
    });

    function deleteEventRecipient(evRecipient) {
        $.ajax({
            type: "POST",
            url: '\/event\/recipient_delete',
            data: {'eventRecipient': evRecipient},
        });
    }

    function addRecipientBlock(checkbox) {
        var form = checkbox.closest('form');
        var checked = checkbox.is(":checked");
        var elId = checkbox.attr('id');
        var label = $('[for=' + elId + ']');
        var name = label.html();
        var recipientBox = $('<div class="recipient-holder recipient-' + elId + '"><span>' + name +
                '</span><button type="button" class=" del-recipient " style="cursor: pointer;" data-recipient=' + elId + '><i class="material-icons">clear</i></button></div>');

        if (checked && $('.recipient-blocks').find('.recipient-event_recipients_' + elId).length < 1) {
            form.find('.recipient-blocks').append(recipientBox);
        } else {
            $('.recipient-' + elId).remove();
            checkbox.prop('checked', false);
        }
        addedRecipients(form);
    }

    function appendEvents(evCountArr) {
        var shownEvents = [];

        $('.checkAppend').remove();
        $('.event-holder').each(function() {
            shownEvents.push(this.id.split('_')[1]);
        });

        $.ajax({
            type: "POST",
            url: '\/event\/list',
            data: {'evCount': evCountArr, 'shownEvents': shownEvents},
            success: function(html) {
                if ($(html).find('.event-row').children().length > 0) {
                    $('.event-row').append($(html).find('.event-row').children());
                    $("<div class='checkAppend' data-ev-count='" + $('.ev-count-cl').length + "' data-gl-ev-count='" + $('.glEv-count').length + "'></div>")
                            .insertAfter($('.event-holder').last());
                    $('.ev-file-link').each(function() {
                        var link = $(this).attr('href');
                        var fileType = link.slice((Math.max(0, link.lastIndexOf(".")) || Infinity) + 1);
                        if ((fileType == "docx") || (fileType == "doc")) {
                            $(this).children().attr({src: '/layout/images/document-word.png'});
                        } else if ((fileType == "jpg") || (fileType == "gif") || (fileType == "png")) {
                            $(this).children().attr({src: '/layout/images/document-image.png'});
                        } else if ((fileType == "xls") || (fileType == "xlsx")) {
                            $(this).children().attr({src: '/layout/images/document-excel.png'});
                        } else if ((fileType == "pdf")) {
                            $(this).children().attr({src: '/layout/images/document-pdf.png'});
                        } else {
                            $(this).children().attr({src: '/layout/images/document.png'});
                        }
                    });
                    init();
                }
                interactiveUrlsForEvents();
            }
        });
    }
    ;

    function urlify(elem) {
        var exp = /(\b(https?|ftp|file):\/\/[-A-Z0-9+&@#\/%?=~_|!:,.;]*[-A-Z0-9+&@#\/%=~_|])/ig;
        var text = elem.find('.event-text').html();
        var evImg = elem.find('.col-md-3.event-image');

        if (exp.test(text)) {
            var text2 = text.replace(exp, "<a href='$1' class='text-link'>$1</a>");
            var url = elem.html().match(exp)[0];

            $($(elem).find('.event-text')).html(text2);

            if (evImg.length > 0) {
                return true;
            }

            $.ajax({
                type: "POST",
                url: '\/event\/get_content',
                data: {url: url},
                success: function(data) {
                    var evLink = elem.find('.ev-link');
                    var hasLink = false;
                    var youtube = false;

                    evLink.prop('href', url);
                    evLink.attr('data-ready', true);

                    $(data).each(function(index) {
                        if ($(this).attr('property') == 'og:site_name' && $(this).attr('content') == 'YouTube' && evImg.length < 1) {
                            youtube = true;
                        }
                        if (youtube && $(this).attr('property') == 'og:video:url') {
                            if ($(this).attr('content').includes('embed')) {
                                hasLink = true;
                                evLink.find('.ev-link-img')
                                        .html('<iframe src="' + $(this).attr('content') + '"></iframe>');
                            }
                        }
                        if ($(this).attr('property') == 'og:image' && elem.find('.link-image').length < 1 && !youtube && evImg.length < 1) {
                            var imgSrc = $(this).attr('content');
                            hasLink = true;
                            evLink.find('.ev-link-img').html('<img class="link-image" src="' + imgSrc + '" style="width:100px;">');
                        }
                        if ($(this).attr('property') == 'og:title' && elem.find('.link-title').length < 1) {
                            var title = $(this).attr('content');
                            hasLink = true;
                            evLink.find('.ev-link-title').html('<span class="link-title">' + title + '</span>');
                        }
                        if (!youtube && $(this).attr('property') == 'og:description' && elem.find('.link-body').length < 1) {
                            var body = $(this).attr('content');
                            hasLink = true;
                            evLink.find('.ev-link-body').html('<span class="link-body">' + body + '</span>');
                        }
                    });

                    if (youtube) {
                        evLink.removeClass('col-md-8');
                        evLink.css('border', 'none');
                    }

                    if (hasLink == true) {
                        elem.find('.text-link')[0].remove();
                        evLink.show();
                    }
                }
            });
        }
    }

    function bindUrlify() {
        $('.event-holder').each(function(index) {
            if ($(this).find('.ev-link').attr('data-ready') == 'false') {
                var textEl = $($(this).children()[2]);
                urlify($(this));
            }
        });
    }

    function deleteFile(target, object) {
        var fileId = $(target).attr('data-to-del');

        if (object == 'image') {
            var question = "Ar\u0020tikrai\u0020norite\u0020i\u0161trinti\u0020\u0161i\u0105\u0020nuotrauk\u0105" + " ?"
        } else {
            var question = "Ar\u0020tikrai\u0020norite\u0020i\u0161trinti\u0020\u0161i\u0105\u0020failas" + " ?"
        }

        if (confirm(question)) {
            $.ajax({
                type: "POST",
                url: '\/event\/file_delete',
                data: {'id': fileId, 'object': object},
                success: function(data) {
                    $(target).parent().remove();
                    $('#' + fileId).remove();
                }
            });

            if (object == 'image') {
                var form = $(target).closest('form');
                form.find('.image-edit-div').hide();
                form.parent().parent().find('.event-text').toggleClass('col-md-8 col-md-12');
                $('#' + fileId).remove();
                $('.event-image-block').append('<img class="ev-img" src="#" alt="your image" style="max-height: 100px;max-width: 200px;display:none;"/>'
                + '<a href="#" class="remove-picture"><i class="material-icons">highlight_off</i></a>');
            }
        }
    }

    function deleteComment(ev) {
        var id = $(ev).attr('data-to-delete');

        $.ajax({
            type: "POST",
            url: '\/event\/comment_delete',
            data: {'id': id},
            success: function(data) {
                var commCount = $(ev).parent().parent().siblings(".media").length;
                $(ev).parents(".event-holder").find(".comment-count").empty().append(commCount);
                $($("#comment_" + id)).fadeOut(300, function() {
                    $(this).remove();
                });
            }
        });
    }

    function readURL(input) {
        var fileType = $(input)[0].files[0].type;
        var form = $(input).parent().parent().parent();
        var image = form.find('.ev-img');

        if (input.files && input.files[0] && image.attr('src') == '#' && imgExt.includes(fileType)) {
            var reader = new FileReader();

            reader.onload = function(e) {
                form.find('.remove-picture').show();
                var inputId = $(input).attr('id');
                $(input).addClass('image-input');
                form.find('.ev-img').show();
                form.find('.ev-img').attr('src', e.target.result);
                $("[data-to-hide=" + inputId + "]").parent().hide();
                setImage = true;
            }

            reader.readAsDataURL(input.files[0]);
        }
    }
</script>
<script>
    function showFileImg(ev) {
        ev.find('.ev-file-link').each(function() {
            var link = $(this).attr('href');
            var fileType = link.slice((Math.max(0, link.lastIndexOf(".")) || Infinity) + 1);
            if ((fileType == "docx") || (fileType == "doc")) {
                $(this).children().attr({src: '/layout/images/document-word.png'});
            } else if ((fileType == "jpg") || (fileType == "gif") || (fileType == "png")) {
                $(this).children().attr({src: '/layout/images/document-image.png'});
            } else if ((fileType == "xls") || (fileType == "xlsx")) {
                $(this).children().attr({src: '/layout/images/document-excel.png'});
            } else if ((fileType == "pdf")) {
                $(this).children().attr({src: '/layout/images/document-pdf.png'});
            } else {
                $(this).children().attr({src: '/layout/images/document.png'});
            }
        });
    }

