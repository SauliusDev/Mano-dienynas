    <script type="text/javascript">

        $(document).on('click', '#help-msg-attach-file', function(e) {
            e.preventDefault();

            $( ".header_mail_file" ).each(function() {
                if (!$(this).val()) {
                    $(this).trigger('click');

                    return false;
                }
            });

            return false;
        });

        $( ".header_mail_file" ).change(function() {
            var fullname = $(this)[0].files[0].name;
            var fileType = fullname.slice((Math.max(0, fullname.lastIndexOf(".")) || Infinity) + 1);
            var name = fullname.substr(0, 25) + "...";
            var src = '';

            if ((fileType == "docx") || (fileType == "doc")) {
                src="/layout/images/document-word.png";
            } else if ((fileType == "jpg") || (fileType == "gif") || (fileType == "png")) {
                src="/layout/images/document-image.png";
            } else if ((fileType == "xls") || (fileType == "xlsx")) {
                src="/layout/images/document-excel.png";
            } else if ((fileType == "pdf")) {
                src="/layout/images/document-pdf.png";
            } else {
                src="/layout/images/document.png";
            }

            $('#uploaded-files').append('<div style="font-size: 14px;"><img src="' + src + '">' + name +
                '<a href="#" class="remove-file" data-remove="' + $(this).attr('data-num') + '"><i class="material-icons" style="font-size: 16px;">highlight_off</i></a></div>');
        });

        $(document).on('click', '.remove-file', function() {
            $('.' + $(this).attr('data-remove')).val('');
            $(this).parent().remove();
        });

        $("#new-pass1").focus(function() {
        $(".passInfo").slideDown();
        });
        $('#new-pass1').keyup(function() {
        var password = $(this).val();
        var validation = 0;
        if ((password.length >= 8)) {
        validation = validation + 1;
        }
        if (password.match(/[0-9]/)) {
        validation = validation + 1;
        }
        if (password.match(/[A-Z]/)) {
        validation = validation + 1;
        }
        if (validation <= 1) {
        $('.progressBarVal').css({'background': 'red', 'width': '30%','transition': 'width 1s ease-in,background 1s'});
        $('#new-pass1').removeClass('greenBorder');
        }
        if (validation == 2) {
        $('.progressBarVal').css({'background-image': 'linear-gradient(to right, red , yellow)', 'width': '65%','transition': 'width 1s ease-in'});
        $('#new-pass1').removeClass('greenBorder');
        }
        if (validation == 3) {
        $('.progressBarVal').css({'background': 'green', 'width': '100%','transition': 'width 1s ease-in,background 1s'});
        $('#new-pass1').addClass('greenBorder');
        $('#new-pass1').on('focusout', function() {
        $(".passInfo").slideUp();
        });
        }

        });
    </script>
    <script>
        if (location.search.split('redirectFrom=')[1] == "toggleEmailSubscription") {
            $('#container_main_inner')
                .append('<ul class="message-list" id="r4" style="display: none;"><li class="message-success">Gaunamų pranešimų siuntimas iš "Manodienynas.lt" sistemos Jums yra išjungtas.</li></ul>');
        }

    </script>
