                <script>
    if (typeof lan_local_module_name === 'undefined') {
        lan_local_module_name = 'none';
    }

    $(function() {
        jQuery.fn.scrollTo = function(elem) {
            if ($(elem).length > 0) {
                $(this).scrollTop($(this).scrollTop() - $(this).offset().top + $(elem).offset().top);
            }
            return this;
        };


        $('.search_roles').keyup(function() {
            var inputText = $(this).val();
            $('#active-user-wrapper').addClass('expanded');
            $('#userblock_extended').find('.school-wrapper, .child-item').each(function() {
                $(this).removeClass('hidden');
                if (inputText != '') {
                    if (($(this).text().toUpperCase().indexOf(inputText.toUpperCase())) <= 0) {
                        $(this).addClass('hidden');
                    }
                }
            });
        });


        $('.additional-school-name.expandable').click(function() {
            $(this).next('.school-users-types').toggleClass('collapsed');
        });

        $('.child-info-wrapper-in-list.current_child').parent().prependTo('.childrens-list');

        var current_role = $('#user-info-wrapper.1 .additional-school-user-type.current_role');

        current_role.prependTo('.schools-block');
        current_role.css('list-style', 'none');
    })

</script>
