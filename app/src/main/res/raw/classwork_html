<script>
    window.onload = function() {
        var currentMenu = $('#navlist li a.current');
        if (!currentMenu.parents('.meniu_li_f').children('.menu_sub').hasClass('opened')) {
            currentMenu.parents('.meniu_li_f').children('.menu_sub').addClass('opened');
        }

        $("#49").click(function(event) {
            const integrra = document.getElementById('49').getAttribute('data-integrra');
            event.preventDefault();

            if (integrra == 0) {
                $(".rights-popup-wrapper").css('display', 'block');
                $(".rights-popup-bg").fadeIn(500);
            } else {
                window.open('https://www.integrra.com/edvs/subsystems/wp/md.php', '_blank');
            }
        });
        $(".rights-reject").click(function() {
            $(".rights-popup-bg").fadeOut(500);
            $(".rights-popup-wrapper2").css('display', 'none');
        });
        document.getElementById('checkIfAgreeWithIntegrra').addEventListener('click', function(e) {
            document.getElementById('btnAgreeWithIntegrra').disabled = !e.target.checked;
        });
        $('#btnAgreeWithIntegrra').click(function() {
            window.open('https://www.integrra.com/edvs/subsystems/wp/md.php', '_blank');
            $(".rights-popup-bg").fadeOut(500);
            $("#49").attr('data-integrra', '1');
        });
    }
</script>

