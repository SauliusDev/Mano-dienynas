    <script>
        function toggleLoggedMenu() {
            $('.logged-menu-btn').toggleClass('change');
            $('.side-container').toggleClass('menu-open');
            $("#top-header-content").find('.sub-open').removeClass('sub-open');
        };

        $(window).ready(function () {
            var bodyColor = $('body').css('background-color');
            if (bodyColor !== "rgb(255, 255, 255)" && bodyColor !== "transparent" && bodyColor !== "rgba(0, 0, 0, 0)") {
                $('#school-name').css('background-color', 'transparent')
                $('#act-user-info-text').css('background-color', 'transparent')
            }
        });
        (function() {
            var topCountDownWidth = $('#top-header-content').innerWidth();

            var countedPos = (topCountDownWidth / 2) - topCountDownWidth;

            $('#top-header-content').css('margin-left', countedPos);
        })();

        $(document).ready(function () {
            var path = window.location.pathname;
            path = path.replace(/\/$/, "");
            path = decodeURIComponent(path);

            $(".top-menu-container a.link").each(function () {
                var href = $(this).attr('href');
                if (path.substring(0, href.length) === href) {
                    $(this).css('background', 'darkgray');
                    $(this).children('i').css('color', '#fff');
                    $(this).find('.inactive_menu_item').hide();
                    $(this).find('.active_menu_item').show();
                }
            });
        });
    </script>

