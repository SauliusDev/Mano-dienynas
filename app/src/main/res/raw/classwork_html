<script>
    var blockTitleWrapper = $('.menu_sub, .menu_no_sub');
    var menuBlocksWithSubsCount = blockTitleWrapper.length;
    (function() {
        /*CLICKS ON MENU BLOCKS TITLES*/
        blockTitleWrapper.on('click', function(e) {
            $(this).toggleClass('opened');
            if ($(this).hasClass('opened')) {
                unpinPinMenuBlock($(this).attr('id'), true);
            } else {
                unpinPinMenuBlock($(this).attr('id'), false);
            }
        });
        /*IF EXIST COOKIE AND MENU BLOCKS WITH SUBMENU*/
        if (document.cookie.indexOf("activesMenuBlocks") >= 0 && menuBlocksWithSubsCount > 0) {
            var cookie = getCookieUnlimited('activesMenuBlocks');
            if (cookie != "") {
                var arr = cookie.split(',');
                /*               $('ul.subnavlist li a').each(function() {
                 if (location.pathname.indexOf($(this).attr('href')) == 0) {
                 console.log(arr);
                 $('.navParentA.menu_sub#' + arr[arr.length - 2]).addClass('opened');
                 } else {
                 console.log('aa');
                 }
                 });*/
                for (var i = 0; i < arr.length; i++) {
                    blockTitleWrapper.each(function(index, element) {
                        if ($(element).attr('id') == arr[arr.length - 2]) {
                            $(element).addClass('opened');
                        }
                    });
                }
            }
        } else {
            setCookieUnlimited('activesMenuBlocks', '', 365);
        }
    })();
    function setMenuHeight() {
        $(document).ready(function() {
            if (null !== document.getElementById("navlist")) {
                var headerHeight = $('header').outerHeight(true);
                var infoWrapperHeight = $('.school-info-wrapper').outerHeight(true);
                var topPosition = headerHeight + infoWrapperHeight + 45;
                document.getElementById("navlist").style.height = "calc(100vh - " + topPosition + "px)";
                $('#navlist').css({"height": "calc(100vh - "+topPosition+"px)", "height": "-moz-calc(100vh - "+topPosition+"px)", "height": "-webkit-calc(100vh - "+topPosition+"px)"});
            }
        });
    }
    setMenuHeight();

</script>
