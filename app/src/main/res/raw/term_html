<script type="text/javascript">
    $(document).ready(function() {
        var isPrintPage = 0;
        function print_doc() {

            var printContents = document.querySelectorAll(".termContainerDiv")[0].innerHTML;
            var originalContents = document.body.innerHTML;
            document.body.innerHTML = printContents;
            $('.standardHeight').css('height', '50px');

            if ($( "th:contains('Egz.')" ).length > 0) {
                $('.a1').attr('colspan', 5);
            }
            else {
                $('.a1').attr('colspan', 4);
            }
            $('[id^=level]').css('width', '35px');
            $('.a1').css('height', '20px');
            $('body').css('background-color', '#fff');
            $(".apr").hide();
            $('.sem').attr('colspan',5);
            $('.sem:last').attr('colspan',3);
            $("body .child-fio").show();
            $('#formTable a').removeClass('beautifulButton');
            window.print();

            //document.body.innerHTML = originalContents;
        }
        if(isPrintPage) {
          print_doc();
        }
    });
</script>


