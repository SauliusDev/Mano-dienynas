<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ManoDienynas" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/primary_green_accent</item>
        <item name="colorPrimaryVariant">@color/primary_variant_green_light</item>
        <item name="colorOnPrimary">@color/secondary_green_dark</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/primary_green_accent</item>
        <item name="colorSecondaryVariant">@color/primary_variant_green_light</item>
        <item name="colorOnSecondary">@color/secondary_green_dark</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/accent_grey_dark</item>
        <!-- Customize your theme here. -->
        <item name="statusBarBackground">@color/accent_grey</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
</resources>