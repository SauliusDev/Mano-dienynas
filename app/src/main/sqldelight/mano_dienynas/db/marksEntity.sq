CREATE TABLE markEntity (
    id INTEGER NOT NULL PRIMARY KEY,
    lesson TEXT,
    teacher TEXT,
    average TEXT,
    markEvent TEXT
);

getMarkById:
SELECT *
FROM markEntity
WHERE id = :id;

getAllMarks:
SELECT *
FROM markEntity;

insertMark:
INSERT OR REPLACE
INTO markEntity
VALUES(?, ?, ?, ?, ?);

deleteMarkById:
DELETE FROM markEntity
WHERE id = :id;

deleteAllMarks:
DELETE FROM markEntity
WHERE id = :id;