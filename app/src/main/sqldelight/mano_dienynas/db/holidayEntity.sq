CREATE TABLE holidayEntity (
    id INTEGER NOT NULL PRIMARY KEY,
    name TEXT,
    rangeStart TEXT,
    rangeEnd TEXT
);

getHolidayById:
SELECT *
FROM holidayEntity
WHERE id = :id;

getAllHoliday:
SELECT *
FROM holidayEntity;

insertHoliday:
INSERT OR REPLACE
INTO holidayEntity
VALUES(?, ?, ?, ?);

deleteHolidayById:
DELETE FROM holidayEntity
WHERE id = :id;

deleteAllHoliday:
DELETE FROM holidayEntity
WHERE id = :id;