CREATE TABLE calendarEntity (
    id INTEGER NOT NULL PRIMARY KEY,
    allDay INTEGER,
    type TEXT,
    start TEXT,
    title TEXT
);

getCalendarById:
SELECT *
FROM calendarEntity
WHERE id = :id;

getAllCalendar:
SELECT *
FROM calendarEntity;

insertCalendar:
INSERT OR REPLACE
INTO calendarEntity
VALUES(?, ?, ?, ?, ?);

deleteCalendarById:
DELETE FROM calendarEntity
WHERE id = :id;

deleteAllCalendar:
DELETE FROM calendarEntity;