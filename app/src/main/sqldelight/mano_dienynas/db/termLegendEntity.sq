CREATE TABLE termLegendEntity (
    id INTEGER NOT NULL PRIMARY KEY,
    abbreviation TEXT,
    description TEXT
);

getTermLegendById:
SELECT *
FROM termLegendEntity
WHERE id = :id;

getAllTermLegend:
SELECT *
FROM termLegendEntity;

insertTermLegend:
INSERT OR REPLACE
INTO termLegendEntity
VALUES(?, ?, ?);

deleteTermLegendById:
DELETE FROM termLegendEntity
WHERE id = :id;

deleteAllTermLegend:
DELETE FROM termLegendEntity;