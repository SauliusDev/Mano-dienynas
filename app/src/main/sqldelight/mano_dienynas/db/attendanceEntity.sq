CREATE TABLE attendanceEntity (
    id INTEGER NOT NULL PRIMARY KEY,
    lessonTitle TEXT,
    teacher TEXT,
    attendance TEXT,
    attendanceRange TEXT
);

getAttendanceById:
SELECT *
FROM attendanceEntity
WHERE id = :id;

getAllAttendance:
SELECT *
FROM attendanceEntity;

insertAttendance:
INSERT OR REPLACE
INTO attendanceEntity
VALUES(?, ?, ?, ?, ?);

deleteAttendanceById:
DELETE FROM attendanceEntity
WHERE id = :id;

deleteAllAttendance:
DELETE FROM attendanceEntity;


